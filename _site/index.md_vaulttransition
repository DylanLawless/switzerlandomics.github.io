    .infra-maturity-stages .infra-cell.is-filled:nth-child(5){ background: var(--infra-red-3); } /* standard (same max) */

    /* upgrade when color-mix is supported: smooth ramp, still max at stage 4 */
    @supports (background: color-mix(in srgb, #000 50%, #fff 50%)){
      .infra-maturity-stages .infra-cell.is-filled:nth-child(1){
        background: var(--infra-red-1);
      }
      .infra-maturity-stages .infra-cell.is-filled:nth-child(2){
        background: color-mix(in srgb, var(--infra-red-1) 40%, var(--infra-red-2) 60%);
      }
      .infra-maturity-stages .infra-cell.is-filled:nth-child(3){
        background: color-mix(in srgb, var(--infra-red-2) 70%, var(--infra-red-3) 30%);
      }
      .infra-maturity-stages .infra-cell.is-filled:nth-child(4){
        background: var(--infra-red-3);
      }
      .infra-maturity-stages .infra-cell.is-filled:nth-child(5){
        background: var(--infra-red-3);
      }
    }

