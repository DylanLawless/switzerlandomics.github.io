    /* fallback first (Safari safe): uses all 3 reds, maxes out at stage 4 */
    .infra-maturity-stages .infra-cell.is-filled:nth-child(1){ background: var(--infra-red-1); } /* concept */
    .infra-maturity-stages .infra-cell.is-filled:nth-child(2){ background: var(--infra-red-2); } /* prototype */
    .infra-maturity-stages .infra-cell.is-filled:nth-child(3){ background: var(--infra-red-2); } /* production */
